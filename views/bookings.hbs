<table class="table table-striped table-sm">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Квартира</th>
      <th scope="col">Заезд</th>
      <th scope="col">Выезд</th>
      <th scope="col">Клиент</th>
      <th scope="col">Телефон</th>
      <th scope="col">Создано</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each bookings}}
      <tr>
        <td>
          <a href="{{this.realtyBookingLink}}">
            {{this.id}}
          </a>
        </td>
        <td>{{this.apartmentId}}</td>
        <td>{{this.beginDate}}</td>
        <td>{{this.endDate}}</td>
        <td>{{this.client.name}}</td>
        <td>{{this.client.tel}}</td>
        <td>{{this.createDate}}</td>
        <td>
          <a
            href="/bookings/{{this.id}}"
            class="btn btn-primary btn-sm"
            tabindex="-1"
            role="button"
          >
            ...
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>